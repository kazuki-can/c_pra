    private void jPanel1MouseClicked(java.awt.event.MouseEvent evt) {                                     
        // TODO add your handling code here:
        count[0]++;
        String item_name = "$" + jLabelprice1.getText();
        food1 = jLabelname1.getText();
        TotalDisplay.append(food1 + "\n");
        itemDisplay.setText(item_name);
    }                                    

