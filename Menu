    private void cancelActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        TotalDisplay.setText("");
        itemDisplay.setText("");
        for(int i=0;i<9;i++){
             count[i] =0;
        }
        
    }                                      

    private void checkActionPerformed(java.awt.event.ActionEvent evt) {                                      
        // TODO add your handling code here:
        String fee;
        TotalDisplay.setText("");
        total_fee = 4.5*count[0]+5.8*count[1]+4.0*count[2]+5.5*count[3]+2.5*(count[4]+count[5])+1.0*(count[6]+count[7]+count[8]);
        fee = String.format("%.1f", total_fee);
        itemDisplay.setText("");
        itemDisplay.setText("$" + fee);
        if(count[0]!=0)TotalDisplay.append(food1 + "   "+"x"+count[0] + "\n");
        if(count[1]!=0)TotalDisplay.append(food2 + "   "+"x"+count[1] + "\n");
        if(count[2]!=0)TotalDisplay.append(food3 + "   "+"x"+count[2] + "\n");
        if(count[3]!=0)TotalDisplay.append(food4 + "   "+"x"+count[3] + "\n");
        if(count[4]!=0)TotalDisplay.append(food5 + "   "+"x"+count[4] + "\n");
        if(count[5]!=0)TotalDisplay.append(food6 + "   "+"x"+count[5] + "\n");
        if(count[6]!=0)TotalDisplay.append(food7 + "   "+"x"+count[6] + "\n");
        if(count[7]!=0)TotalDisplay.append(food8 + "   "+"x"+count[7] + "\n");
        if(count[8]!=0)TotalDisplay.append(food9 + "   "+"x"+count[8] + "\n");
        
        
                 
        
                     
             
            
            
        
        
    }                                     

