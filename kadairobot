import math
import numpy as np
actions = [[-30,100],[50,30],[-40,60],[30,30]]

x = 0.0
y = 0.0
a = 0
xlist = [x]
ylist = [y]
for i in range(4):
  a = a + actions[i][0]
  y = x + actions[i][1]/ math.sin(a)
  x = y + actions[i][1]/ math.cos(a)
  xlist.append(x)
  ylist.append(y)
  print(x)
  print(y)
  
angle1 = math.atan2(x,y)
angle2 = np.rad2deg(angle1)
print(angle2)
plt.plot(xlist,ylist)

